{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"sourceMap": true,
		"outDir": "../out/src",

		"noImplicitAny": false,
		"removeComments": false,
		"noUnusedLocals": false,
		"noImplicitThis": false,
		"inlineSourceMap": false,
		"preserveConstEnums": false,
		"strictNullChecks": false,
		"noUnusedParameters": false
	}
}
